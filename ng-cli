ng g component -cd OnPush -m bqs-square chart-js
ng g component -cd OnPush -m bqs-square data-table

ng g component -cd OnPush -m bqs-square input-date
ng g component -cd OnPush -m bqs-square input-value
ng g component -cd OnPush -m bqs-square input-select

ng g component -cd OnPush -ve None bqs-square-sidenav

ng g module -r bqs-admin

ng g module -r bqs-query
ng g component -cd OnPush -m bqs-query bqs-query-list
ng g component -cd OnPush -m bqs-query bqs-query-edit

ng g module -r bqs-report
ng g component -cd OnPush -m bqs-report bqs-report-list
ng g component -cd OnPush -m bqs-report bqs-report-edit

ng g module -r bqs-key-value
ng g component -cd OnPush -m bqs-key-value bqs-global-key-list
ng g component -cd OnPush -m bqs-key-value bqs-global-key-edit
ng g component -cd OnPush -m bqs-key-value bqs-global-key-view


